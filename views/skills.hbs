<body style="background-image: url('{{backgroundImg}}');">
  <section class="m-5">

    <h3>{{title}}</h3>

    {{#each skills}}
      <div class="skill-section">
        <h4>{{this.type}}</h4>
        <ul class="skill-list">
          {{#each this.lang}}
            <li>{{this}}</li>
          {{/each}}
        </ul>
      </div>
    {{/each}}

  </section>
</body>
